<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gdeastriver.datasync.dao.DepartsDao">

    <select id="selectAll" resultType="com.gdeastriver.datasync.pojo.Departs">
      select  depart_id,depart_name,group_id,inside_id,principal,emp_prefix  from  Departs
    </select>

    <select id="countAll" resultType="int">
        select count(*) from Departs
    </select>

</mapper>